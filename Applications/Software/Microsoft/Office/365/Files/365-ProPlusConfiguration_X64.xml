<Configuration>
  <Add OfficeClientEdition="64" Channel="Monthly" AllowCdnFallback="TRUE" ForceUpgrade="TRUE">
    <Product ID="O365ProPlusRetail">
      <Language ID="en-us" />
      <ExcludeApp ID="Groove" />
      <ExcludeApp ID="OneDrive" />
      <ExcludeApp ID="Lync" />
      <ExcludeApp ID="Teams" />
    </Product>
  </Add>
  <Property Name="SharedComputerLicensing" Value="0" />
  <Property Name="PinIconsToTaskbar" Value="TRUE" />
  <Property Name="SCLCacheOverride" Value="0" />
  <Updates Enabled="TRUE" Channel="Monthly" />
  <RemoveMSI All="TRUE" />
  <AppSettings>
    <User Key="software\microsoft\office\16.0\common\toolbars" Name="screentipscheme" Value="0" Type="REG_DWORD" App="office16" Id="L_ShowScreenTips" />
  </AppSettings>
</Configuration> 
